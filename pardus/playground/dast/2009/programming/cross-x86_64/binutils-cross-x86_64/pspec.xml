<?xml version="1.0" ?>
<!DOCTYPE PISI SYSTEM "http://www.pardus.org.tr/projeler/pisi/pisi-spec.dtd">
<PISI>
    <Source>
        <Name>binutils-cross-x86_64</Name>
        <Homepage>http://sources.redhat.com/binutils/</Homepage>
        <Packager>
            <Name>David Stegbauer</Name>
            <Email>daaste@gmail.com</Email>
        </Packager>
        <License>GPLv2</License>
        <IsA>app:console</IsA>
        <Summary>Cross Assembler and linker tools for x86_64</Summary>
        <Description>Contains commands like as, ld, nm, objdump, ar, ranlib for compiling assembly code to the machine language, and for linking and manipulating the binary object files for cross compiling to x86_64.</Description>
        <Archive sha1sum="e589e02898ae50d38b49966785201183371d84b9" type="tarbz2">http://www.kernel.org/pub/linux/devel/binutils/binutils-2.19.51.0.9.tar.bz2</Archive>
        <Patches>
            <!--  makes Binutils search in both the lib and lib64 directories in a multilib environment -->
            <Patch level="1">binutils-2.19.51-genscripts_multilib-1.patch</Patch>
        </Patches>
    </Source>

    <Package>
        <Name>binutils-cross-x86_64</Name>
        <Files>
            <Path fileType="config">/etc</Path>
            <Path fileType="executable">/opt/cross-x86_64/bin</Path>
            <Path fileType="executable">/opt/cross-x86_64/x86_64-unknown-linux-gnu/bin</Path>
            <Path fileType="library">/opt/cross-x86_64/lib</Path>
            <Path fileType="library">/opt/cross-x86_64/x86_64-unknown-linux-gnu/lib</Path>
            <Path fileType="header">/opt/cross-x86_64/include</Path>
            <Path fileType="man">/opt/cross-x86_64/man</Path>
        </Files>
        <!--AdditionalFiles>
            <AdditionalFile owner="root" permission="0644" target="/etc/env.d/05binutils-avr">05binutils-avr</AdditionalFile>
        </AdditionalFiles-->
    </Package>
    
    <History>
        <Update release="1">
            <Date>2009-09-08</Date>
            <Version>2.19.51.0.9</Version>
            <Comment>First release.</Comment>
            <Name>David Stegbauer</Name>
            <Email>daaste@gmail.com</Email>
        </Update>
    </History>
</PISI>
