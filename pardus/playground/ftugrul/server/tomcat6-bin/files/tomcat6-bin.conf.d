# Config file for /etc/init.d/tomcat6-bin

CATALINA_HOME=/opt/tomcat6-bin
CATALINA_BASE=/opt/tomcat6-bin
CATALINA_TMPDIR=/opt/tomcat6-bin/temp
CLASSPATH=/opt/sun-jdk/lib/tools.jar:/opt/tomcat6-bin/bin/bootstrap.jar:/opt/tomcat6-bin/lib
LOGGING_CONFIG=/opt/tomcat6-bin/conf/logging.properties
JAVA_OPTS=org.apache.juli.ClassLoaderLogManager
CATALINA_PID=/var/run/tomcat6-bin.pid
