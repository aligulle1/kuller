<?xml version="1.0" ?>
<!DOCTYPE PISI SYSTEM "http://www.pardus.org.tr/projeler/pisi/pisi-spec.dtd">
<PISI>
    <Source>
        <Name>qpspmanager</Name>
    <Homepage>http://qpspmanager.sourceforge.net</Homepage>
        <Packager>
            <Name>Mustafa Günay</Name>
            <Email>mgunay@pardusman.org</Email>
        </Packager>
        <License>GPLv2</License>
        <IsA>app:gui</IsA>
        <Summary>A PBP File manager for Linux</Summary>
        <Description>It's intended to use as a user friendly GUI to the known tedious pack and unpack processes of the PSP PBP files, directory generation and further copy into the PSP, in order to execute those binary files in PSPs with firmware 1.50</Description>
        <Archive sha1sum="8aa73e4ecca2ceb598054d4895ac4372f34aa3ad" type="targz">mirrors://sourceforge/qpspmanager/qpspmanager-2.0.2.tar.gz</Archive>
        <BuildDependencies>
            <Dependency>qt4</Dependency>
        </BuildDependencies>
    </Source>

    <Package>
        <Name>qpspmanager</Name>
        <RuntimeDependencies>
            <Dependency>qt4</Dependency>
        </RuntimeDependencies>
        <Files>
            <Path fileType="executable">/usr/bin</Path>
            <Path fileType="data">/usr/share/applications</Path>
            <Path fileType="data">/usr/share/pixmaps</Path>
        </Files>
        <AdditionalFiles>
            <AdditionalFile permission="0644" target="/usr/share/pixmaps/qpspmanager.png">qpspmanager.png</AdditionalFile>
            <AdditionalFile permission="0644" target="/usr/share/applications/qpspmanager.desktop">qpspmanager.desktop</AdditionalFile>
        </AdditionalFiles>
    </Package>

    <History>
        <Update release="1">
            <Date>2007-03-21</Date>
            <Version>2.0.2</Version>
            <Comment>First release.</Comment>
            <Name>Mustafa Günay</Name>
            <Email>mgunay@pardusman.org</Email>
        </Update>
    </History>
</PISI>
