<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-nmea">
    <sub-class-of type="text/plain"/>
    <comment xml:lang="en">NMEA data</comment>
    <comment xml:lang="de">NMEA-Daten</comment>
    <glob pattern="*.nmea" />
    <glob pattern="*.nma" />
    <glob pattern="*.nme" />
    <icon name="merkaartor" />
  </mime-type>
  <mime-type type="application/x-noniplot">
    <sub-class-of type="text/plain"/>
    <comment xml:lang="en">Noni GPSPlot data</comment>
    <comment xml:lang="de">Noni GPSPlot-Daten</comment>
    <glob pattern="*.ngt" />
    <icon name="merkaartor" />
  </mime-type>
  <mime-type type="application/x-osb">
    <comment xml:lang="en">OpenStreetMap binary data</comment>
    <comment xml:lang="de">OpenStreetMap-Bin√§rdaten</comment>
    <glob pattern="*.osb" />
    <icon name="merkaartor" />
  </mime-type>
  <mime-type type="application/x-osm+xml">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">OpenStreetMap data</comment>
    <comment xml:lang="de">OpenStreetMap-Daten</comment>
    <magic priority="80">
      <match value="&lt;osm" type="string" offset="0:512"/>
    </magic>
    <glob pattern="*.osm" />
    <icon name="merkaartor" />
  </mime-type>
  <mime-type type="application/x-mdc+xml">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">Merkaartor data</comment>
    <comment xml:lang="de">Merkaartor-Daten</comment>
    <magic priority="90">
      <match value="&lt;MerkaartorDocument" type="string" offset="0:256"/>
    </magic>
    <glob pattern="*.mdc" />
    <icon name="merkaartor" />
  </mime-type>
  <mime-type type="application/x-kml+xml">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">KML track</comment>
    <comment xml:lang="de">KML-Track</comment>
    <magic priority="90">
      <match value="&lt;kml" type="string" offset="0:256"/>
    </magic>
    <glob pattern="*.kml" />
    <glob pattern="*.KML" />
    <root-XML namespaceURI="http://www.opengis.net/kml/2.2" localName="kml"/>
    <icon name="merkaartor" />
  </mime-type>
  <mime-type type="application/x-gpx+xml">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">GPX track</comment>
    <comment xml:lang="de">GPX-Track</comment>
    <magic priority="90">
      <match value="&lt;gpx" type="string" offset="0:256"/>
    </magic>
    <glob pattern="*.gpx" />
    <glob pattern="*.GPX" />
    <root-XML namespaceURI="http://www.topografix.com/GPX/1/0" localName="gpx"/>
    <icon name="merkaartor" />
  </mime-type>
  <mime-type type="application/x-merkaartor-preset">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">Merkaartor preset</comment>
    <comment xml:lang="de">Merkaator-Objektvorlage</comment>
    <magic priority="80">
      <match value="&lt;templates" type="string" offset="0:256"/>
    </magic>
    <glob pattern="*.mat" />
    <root-XML namespaceURI="http://www.merkaartor.org/tagtemplate/0.1/" localName="templates"/>
    <icon name="merkaartor" />
  </mime-type>
  <mime-type type="application/x-merkaartor-style">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">Merkaartor mappaint style</comment>
    <comment xml:lang="de">Merkaartor-Zeichenvorlage</comment>
    <magic priority="80">
      <match value="&lt;mapStyle" type="string" offset="0:256"/>
    </magic>
    <glob pattern="*.mas" />
    <icon name="merkaartor" />
  </mime-type>
  <mime-type type="application/x-merkaartor-list">
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">Merkaartor data list</comment>
    <comment xml:lang="de">Merkaartor-Datenliste</comment>
    <magic priority="80">
      <match value="&lt;MerkaartorList" type="string" offset="0:256"/>
    </magic>
    <!-- <glob pattern="*.xml" /> -->
    <icon name="merkaartor" />
  </mime-type>
  <mime-type type="application/x-merkaartor-wmslist">
    <sub-class-of type="application/x-merkaartor-list"/>
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">Merkaartor WMS list</comment>
    <comment xml:lang="de">Merkaartor-WMS-Liste</comment>
    <magic priority="85">
      <match value="&lt;MerkaartorList" type="string" offset="0:256">
        <match value="&lt;WmsServers" type="string" offset="0:256"/>
      </match>
    </magic>
    <!-- <glob pattern="*.xml" /> -->
    <icon name="merkaartor" />
  </mime-type>
  <mime-type type="application/x-merkaartor-tmslist">
    <sub-class-of type="application/x-merkaartor-list"/>
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">Merkaartor TMS list</comment>
    <comment xml:lang="de">Merkaartor-TMS-Liste</comment>
    <magic priority="85">
      <match value="&lt;MerkaartorList" type="string" offset="0:256">
        <match value="&lt;TmsServers" type="string" offset="0:256"/>
      </match>
    </magic>
    <!-- <glob pattern="*.xml" /> -->
    <icon name="merkaartor" />
  </mime-type>
  <mime-type type="application/x-merkaartor-projectionslist">
    <sub-class-of type="application/x-merkaartor-list"/>
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">Merkaartor projections list</comment>
    <comment xml:lang="de">Merkaartor-Projektionsliste</comment>
    <magic priority="85">
      <match value="&lt;MerkaartorList" type="string" offset="0:256">
        <match value="&lt;Projections" type="string" offset="0:256"/>
      </match>
    </magic>
    <!-- <glob pattern="*.xml" /> -->
    <icon name="merkaartor" />
  </mime-type>
  <mime-type type="application/x-merkaartor-bookmarkslist">
    <sub-class-of type="application/x-merkaartor-list"/>
    <sub-class-of type="application/xml"/>
    <comment xml:lang="en">Merkaartor bookmarks</comment>
    <comment xml:lang="de">Merkaartor-Lesezeichen</comment>
    <magic priority="85">
      <match value="&lt;MerkaartorList" type="string" offset="0:256">
        <match value="&lt;Bookmarks" type="string" offset="0:256"/>
      </match>
    </magic>
    <!-- <glob pattern="*.xml" /> -->
    <icon name="merkaartor" />
  </mime-type>
</mime-info>
