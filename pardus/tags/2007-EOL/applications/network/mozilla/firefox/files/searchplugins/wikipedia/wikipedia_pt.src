<SEARCH
    version = "7.1"
    name="Wikipédia"
    description="a enciclopédia livre"
    method="GET"
    action="http://pt.wikipedia.org/wiki/Special:Search"
    queryCharset="UTF-8"
    searchForm="http://pt.wikipedia.org/"
    routeType="internet"
>

<input name="search" user>
<input name="go" value="Go">
<input name="sourceid" value="Mozilla-search">
<inputnext name="offset" factor="20">
<inputprev name="offset" factor="20">

<interpret
    browserResultType="result"
    charset="UTF-8"
    resultListStart="</h2>"
    resultListEnd="<p>"
    resultItemStart="<li>"
    resultItemEnd="</li>"
>

</search>