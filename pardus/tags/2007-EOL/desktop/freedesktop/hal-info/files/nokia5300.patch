# Description: Add Nokia 5300 music player
# Ubuntu: https://bugs.launchpad.net/bugs/103831
# Upstream: https://bugs.freedesktop.org/show_bug.cgi?id=12976
--- hal-info-20080215/fdi/information/10freedesktop/10-usb-music-players.fdi	2008-02-28 15:59:57.000000000 +0100
+++ hal-info-20080215.new/fdi/information/10freedesktop/10-usb-music-players.fdi	2008-02-28 16:03:21.000000000 +0100
@@ -85,6 +85,13 @@
           <match key="@storage.originating_device:usb.product_id" int_outof="0x0433;0x0434">
             <merge key="storage.requires_eject" type="bool">true</merge>
           </match>
+          <!-- Nokia 5300 -->
+          <match key="@storage.originating_device:usb.product_id" int="0x4b9">
+            <merge key="storage.model" type="string">5300</merge>
+            <merge key="portable_audio_player.type" type="string">generic</merge>
+            <merge key="portable_audio_player.access_method" type="string">storage</merge>
+            <append key="portable_audio_player.audio_folders" type="strlist">Music/</append>
+          </match>
         </match>
 
         <!-- MPIO HD300 Player -->
