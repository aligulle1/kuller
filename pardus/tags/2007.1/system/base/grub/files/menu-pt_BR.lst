default 0
timeout 10
splashimage (cd)/boot/grub/splash.xpm.gz
background 10333C

title=%(title)s
root (cd)
kernel (cd)/boot/%(kernel)s root=/dev/ram0 video=vesafb:nomtrr,pmipal,ywrap,1024x768-32@60 splash=silent,fadein,theme:pardus console=tty2 mudur=livecd,language:pt_BR quiet
initrd (cd)/boot/%(initramfs)s

title=%(title)s (Modo Grafico Padrao)
root (cd)
kernel (cd)/boot/%(kernel)s root=/dev/ram0 video=vesafb:off mudur=livecd,language:pt_BR quiet
initrd (cd)/boot/%(initramfs)s

title=%(title)s (Opcao Minima)
root (cd)
kernel (cd)/boot/%(kernel)s root=/dev/ram0 video=vesafb:off acpi=off apm=off nolapic noapic maxcpus=0 mudur=livecd,language:pt_BR
initrd (cd)/boot/%(initramfs)s

title = Teste de Memoria
root (cd)
kernel (cd)/boot/memtest.bin

title=Continuar do Disco
rootnoverify (hd0)
chainloader +1

title=Other Languages
configfile (cd)/boot/grub/menu.lst

