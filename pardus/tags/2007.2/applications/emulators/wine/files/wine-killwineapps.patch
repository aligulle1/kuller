diff -Naur wine-0.9.5.orig/tools/killwineapps/killwineapps.sh wine-0.9.5/tools/killwineapps/killwineapps.sh
--- wine-0.9.5.orig/tools/killwineapps/killwineapps	1970-01-01 02:00:00.000000000 +0200
+++ wine-0.9.5/tools/killwineapps/killwineapps	2006-01-22 20:22:00.000000000 +0200
@@ -0,0 +1,16 @@
+#! /bin/sh
+
+export TEXTDOMAINDIR=/usr/share/locale
+export TEXTDOMAIN=killwineapps
+
+. gettext.sh &> /dev/null
+
+kdialog --title        "`eval_gettext "Terminate Wine Processes"`" \
+        --warningyesno "`eval_gettext "\
+Are you sure you want to terminate all wine processes?"`" || exit
+
+kdialog --passivepopup "`eval_gettext "Terminating wine processes..."`" &
+dlgpid=$!
+wineserver -kw
+kill $! 1>/dev/null
+kdialog --passivepopup "`eval_gettext "All wine processes terminated"`" 1
