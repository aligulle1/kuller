default 0
timeout 10
splashimage /boot/grub/splash.xpm.gz
background 10333C

title=%(title)s
root 
kernel /boot/%(kernel)s root=/dev/ram0 video=vesafb:nomtrr,pmipal,ywrap,1024x768-32@60 splash=silent,fadein,theme:pardus console=tty2 mudur=livecd,language:es quiet
initrd /boot/%(initramfs)s

title=%(title)s (Modo grafico estandar)
root 
kernel /boot/%(kernel)s root=/dev/ram0 video=vesafb:off mudur=livecd,language:es quiet
initrd /boot/%(initramfs)s

title=%(title)s (Opcion minima)
root 
kernel /boot/%(kernel)s root=/dev/ram0 video=vesafb:off acpi=off apm=off nolapic noapic maxcpus=0 mudur=livecd,language:es
initrd /boot/%(initramfs)s

title = Prueba de Memoria
root 
kernel /boot/memtest.bin

title=Continuar desde el Disco rigido
rootnoverify (hd0)
chainloader +1

title=Other Languages
configfile /boot/grub/menu.lst

