<SEARCH
    version = "7.1"
    name="PardusWiki (Türkçe)"
    description="PardusWiki"
    method="GET"
    action="http://tr.pardus-wiki.org/Special:Search"
    queryCharset="UTF-8"
    searchForm="http://tr.pardus-wiki.org/"
    routeType="internet"
>

<input name="search" user>
<input name="go" value="Go">
<input name="sourceid" value="Mozilla-search">
<inputnext name="offset" factor="20">
<inputprev name="offset" factor="20">

<interpret
    browserResultType="result"
    charset="UTF-8"
    resultListStart="</h2>"
    resultListEnd="<p>"
    resultItemStart="<li>"
    resultItemEnd="</li>"
>

</search>
