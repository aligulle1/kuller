            <Dependency>kdelibs</Dependency>
        </BuildDependencies>
    </Source>

    <Package>
        <Name>autostart</Name>
        <RuntimeDependencies>
            <Dependency>kdelibs</Dependency>
        </RuntimeDependencies>
        <Files>
            <Path fileType="library">/usr/kde/3.5/lib/kde3</Path>
            <Path fileType="data">/usr/kde/3.5/share/doc</Path>
            <Path fileType="data">/usr/kde/3.5/share/applications</Path>
            <Path fileType="localedata">/usr/kde/3.5/share/locale</Path>
        </Files>
        <AdditionalFiles>
             <AdditionalFile target="/usr/kde/3.5/share/applications/kde/tasmaautostart.desktop" permission="0644">tasmaautostart.desktop</AdditionalFile>
        </AdditionalFiles>
    </Package>

