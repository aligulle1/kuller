default 0
timeout 10
splashimage (cd)/boot/grub/splash.xpm.gz
background 10333C

title=%(title)s
root (cd)
kernel (cd)/boot/%(kernel)s root=/dev/ram0 video=vesafb:nomtrr,pmipal,ywrap,1024x768-32@60 splash=silent,fadein,theme:pardus console=tty2 mudur=livecd,language:nl quiet
initrd (cd)/boot/%(initramfs)s

title=%(title)s (Standaard Grafische Modus)
root (cd)
kernel (cd)/boot/%(kernel)s root=/dev/ram0 video=vesafb:off mudur=livecd,language:nl quiet
initrd (cd)/boot/%(initramfs)s

title=%(title)s (Minimale Opties)
root (cd)
kernel (cd)/boot/%(kernel)s root=/dev/ram0 video=vesafb:off acpi=off apm=off nolapic noapic maxcpus=0 mudur=livecd,language:nl
initrd (cd)/boot/%(initramfs)s

title = Geheugentest
root (cd)
kernel (cd)/boot/memtest.bin

title=Doorgaan Vanaf Harde Schijf
rootnoverify (hd0)
chainloader +1

title=Other Languages
configfile (cd)/boot/grub/menu-all.lst

