prefix=/usr
exec_prefix=/usr
libdir=/usr/lib/MozillaFirefox
includedir=/usr/lib/MozillaFirefox/include
idldir=/usr/lib/MozillaFirefox/idl

Name: XPCOM
Description: The Mozilla Cross Platform Component Library
Version: 3.0
Requires: nspr >= 4.7.0
Libs: -L${libdir} -lxpcom -Wl,-R${libdir}
Cflags: -I${includedir} -I${includedir}/xpcom -I${includedir}/string
