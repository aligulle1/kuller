    <Package>
        <Name>gambas2-runtime</Name>
        <IsA>app:console</IsA>
        <IsA>library</IsA>
        <Summary>Runtime environment for gambas</Summary>
        <RuntimeDependencies>
            <Dependency>libffi</Dependency>
        </RuntimeDependencies>
        <Files>
            <!-- interpreter -->
            <Path fileType="executable">/usr/bin/gbx2</Path>
            <Path fileType="executable">/usr/bin/gbr2</Path>

            <!-- internal component -->
            <Path fileType="data">/usr/share/gambas2/info/gb.info</Path>
            <Path fileType="data">/usr/share/gambas2/info/gb.list</Path>
            <Path fileType="data">/usr/lib/gambas2/gb.component</Path>

            <!-- documentation -->
            <Path fileType="doc">/usr/share/doc</Path>

            <!-- gb.debug component -->
            <Path fileType="data">/usr/share/gambas2/info/gb.debug.info</Path>
            <Path fileType="data">/usr/share/gambas2/info/gb.debug.list</Path>
            <Path fileType="data">/usr/lib/gambas2/gb.debug.component</Path>
            <Path fileType="library">/usr/lib/gambas2/gb.debug.[ls][ao]</Path>
            <Path fileType="library">/usr/lib/gambas2/gb.debug.so.*</Path>

            <!-- gb.eval component -->
            <Path fileType="data">/usr/share/gambas2/info/gb.eval.info</Path>
            <Path fileType="data">/usr/share/gambas2/info/gb.eval.list</Path>
            <Path fileType="data">/usr/lib/gambas2/gb.eval.component</Path>
            <Path fileType="library">/usr/lib/gambas2/gb.eval.[ls][ao]</Path>
            <Path fileType="library">/usr/lib/gambas2/gb.eval.so.*</Path>

            <!-- gb.draw component -->
            <Path fileType="library">/usr/lib/gambas2/gb.draw.[ls][ao]</Path>
            <Path fileType="library">/usr/lib/gambas2/gb.draw.so.*</Path>

            <!-- mime type -->
            <Path fileType="data">/usr/share/mime/packages/application-x-gambas.xml</Path>
            <Path fileType="data">/usr/share/icons/hicolor/*/mimetypes/application-x-gambas.png</Path>
        </Files>
    </Package>

