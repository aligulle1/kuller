        <IsA>app:gui</IsA>
        <Summary>Firefox Web Browser</Summary>
        <Description>It is more secure and faster to browse the web with Firefox web browser. You can personalize your web browser with many specifications that is not enough to explain in two sentences.</Description>
        <Archive sha1sum="ff4ace5a36a3750d7d3d4946578ae7f4fea72371" type="tarbz2">http://cekirdek.pardus.org.tr/~gokcen/files/firefox-3.0-pardus.tar.bz2</Archive>
        <BuildDependencies>
            <Dependency>xorg-server</Dependency>
            <Dependency versionFrom="2.5.0">fontconfig</Dependency>
            <Dependency versionFrom="1.0.0">libmng</Dependency>
            <Dependency versionFrom="1.2.24">libpng</Dependency>
            <Dependency versionFrom="1.17">lcms</Dependency>
            <Dependency versionFrom="2.8.7">gtk2</Dependency>
            <Dependency versionFrom="1.10.0">pango</Dependency>
            <Dependency versionFrom="1.6.0">cairo</Dependency>
            <Dependency versionFrom="0.8.0">libIDL</Dependency>
            <Dependency versionFrom="3.12_pre20080321">nss</Dependency>
            <Dependency versionFrom="4.7.0_pre20080321">nspr</Dependency>
            <Dependency versionFrom="1.5.8">cairo</Dependency>
            <Dependency>startup-notification</Dependency>
            <Dependency>dbus-glib</Dependency>
        </BuildDependencies>
        <Patches>
            <!-- default config values -->
            <Patch>mozconfig.patch</Patch>

            <!-- No automatic update -->
            <Patch>suse/firefox-no-update.patch</Patch>

            <!-- No default browser patch -->
            <Patch>suse/firefox-appname.patch</Patch>

            <!-- Use os.locale -->
            <Patch>suse/locale.patch</Patch>

            <Patch>gcc43.patch</Patch>
            <Patch level="1">pathmax.patch</Patch>

            <!-- reload new plugins -->
            <Patch level="1">ubuntu/bzXXX_reload_new_plugins.patch</Patch>

            <!-- use stdout for missing file warning -->
            <Patch level="1">ubuntu/installer_use_stdout_for_missing_files.patch</Patch>

            <!-- zemberek support for tr-TR -->
            <Patch level="1">pardus/zemberek-firefox3.patch</Patch>

            <Patch>nss-header-not-found.patch</Patch>

        </Patches>
    </Source>

    <Package>
        <Name>firefox</Name>
        <RuntimeDependencies>
            <Dependency>xorg-server</Dependency>
            <Dependency versionFrom="2.5.0">fontconfig</Dependency>
            <Dependency versionFrom="1.0.0">libmng</Dependency>
            <Dependency versionFrom="1.2.24">libpng</Dependency>
            <Dependency versionFrom="1.17">lcms</Dependency>
            <Dependency versionFrom="2.8.7">gtk2</Dependency>
            <Dependency versionFrom="1.10.0">pango</Dependency>
            <Dependency versionFrom="1.6.0">cairo</Dependency>
            <Dependency versionFrom="0.8.0">libIDL</Dependency>
            <Dependency versionFrom="3.12_pre20080321">nss</Dependency>
            <Dependency versionFrom="4.7.0_pre20080321">nspr</Dependency>
            <Dependency versionFrom="1.5.8">cairo</Dependency>
            <Dependency>startup-notification</Dependency>
            <Dependency>dbus-glib</Dependency>
        </RuntimeDependencies>
        <Files>
            <Path fileType="config">/etc/</Path>
            <Path fileType="executable">/usr/bin</Path>
            <Path fileType="executable">/usr/lib/MozillaFirefox</Path>
            <Path fileType="executable">/usr/libexec</Path>
            <Path fileType="data">/usr/lib/pkgconfig</Path>
            <Path fileType="data">/usr/share/applications</Path>
            <Path fileType="data">/usr/share/pixmaps</Path>
            <Path fileType="doc">/usr/share/doc</Path>
        </Files>
        <AdditionalFiles>
            <!-- wrappers -->
            <AdditionalFile target="/usr/bin/firefox" permission="0755" owner="root">pardus/firefox</AdditionalFile>

            <!-- Icons -->
            <AdditionalFile target="/usr/share/pixmaps/firefox.png" permission="0644" owner="root">pardus/firefox.png</AdditionalFile>

            <!-- env.d -->
            <AdditionalFile target="/etc/env.d/10MozillaFirefox" permission="0644" owner="root">pardus/10MozillaFirefox</AdditionalFile>

            <!-- Desktop file -->
            <AdditionalFile target="/usr/share/applications/mozillafirefox.desktop" permission="0644" owner="root">pardus/mozillafirefox.desktop</AdditionalFile>

            <!-- Corrected *.pc files and config -->
            <AdditionalFile target="/usr/lib/pkgconfig/firefox-js.pc" permission="0644" owner="root">pc/firefox-js.pc</AdditionalFile>
            <AdditionalFile target="/usr/lib/pkgconfig/firefox-plugin.pc" permission="0644" owner="root">pc/firefox-plugin.pc</AdditionalFile>
            <AdditionalFile target="/usr/lib/pkgconfig/firefox-xpcom.pc" permission="0644" owner="root">pc/firefox-xpcom.pc</AdditionalFile>

            <!-- Default options -->
            <AdditionalFile target="/usr/lib/MozillaFirefox/defaults/pref/firefox-pardus.js" permission="0644" owner="root">pardus/default-prefs.js</AdditionalFile>
            <AdditionalFile target="/usr/lib/MozillaFirefox/defaults/pref/firefox-l10n.js" permission="0644" owner="root">pardus/firefox-l10n.js</AdditionalFile>
            <AdditionalFile target="/usr/lib/MozillaFirefox/browserconfig.properties" permission="0644" owner="root">pardus/browserconfig.properties</AdditionalFile>

            <!-- Uluzilla search plugin -->
            <AdditionalFile target="/usr/lib/MozillaFirefox/searchplugins/uluzilla.xml" permission="0644" owner="root">searchplugins/uluzilla.xml</AdditionalFile>
            <!-- Google linux search plugin -->
            <AdditionalFile target="/usr/lib/MozillaFirefox/searchplugins/googlelinux.xml" permission="0644" owner="root">searchplugins/googlelinux.xml</AdditionalFile>

            <!-- Wikipedia search plugins -->
            <AdditionalFile target="/usr/lib/MozillaFirefox/pardus/wikipedia_tr.xml" permission="0644" owner="root">searchplugins/wikipedia/wikipedia_tr.xml</AdditionalFile>
            <AdditionalFile target="/usr/lib/MozillaFirefox/pardus/wikipedia_en.xml" permission="0644" owner="root">searchplugins/wikipedia/wikipedia_en.xml</AdditionalFile>
            <AdditionalFile target="/usr/lib/MozillaFirefox/pardus/wikipedia_nl.xml" permission="0644" owner="root">searchplugins/wikipedia/wikipedia_nl.xml</AdditionalFile>
            <AdditionalFile target="/usr/lib/MozillaFirefox/pardus/wikipedia_pt.xml" permission="0644" owner="root">searchplugins/wikipedia/wikipedia_pt.xml</AdditionalFile>

            <!-- Pardus-Wiki search plugins -->
            <AdditionalFile target="/usr/lib/MozillaFirefox/pardus/pardus-wiki_tr.xml" permission="0644" owner="root">searchplugins/pardus-wiki/pardus-wiki_tr.xml</AdditionalFile>
            <AdditionalFile target="/usr/lib/MozillaFirefox/pardus/pardus-wiki_en.xml" permission="0644" owner="root">searchplugins/pardus-wiki/pardus-wiki_en.xml</AdditionalFile>
            <AdditionalFile target="/usr/lib/MozillaFirefox/pardus/pardus-wiki_nl.xml" permission="0644" owner="root">searchplugins/pardus-wiki/pardus-wiki_nl.xml</AdditionalFile>
            <AdditionalFile target="/usr/lib/MozillaFirefox/pardus/pardus-wiki_pt.xml" permission="0644" owner="root">searchplugins/pardus-wiki/pardus-wiki_pt.xml</AdditionalFile>

            <!-- Default bookmarks -->
            <AdditionalFile target="/usr/lib/MozillaFirefox/pardus/bookmarks-tr.html" permission="0644" owner="root">pardus/pardus_bookmark-tr.html</AdditionalFile>
            <AdditionalFile target="/usr/lib/MozillaFirefox/pardus/bookmarks-en.html" permission="0644" owner="root">pardus/pardus_bookmark-en.html</AdditionalFile>
            <AdditionalFile target="/usr/lib/MozillaFirefox/pardus/bookmarks-nl.html" permission="0644" owner="root">pardus/pardus_bookmark-nl.html</AdditionalFile>

            <!-- KMail Wrapper -->
            <AdditionalFile target="/usr/bin/kmail-firefox" permission="0755" owner="root">kmail-firefox</AdditionalFile>
        </AdditionalFiles>
        <Provides>
            <COMAR script='package.py'>System.Package</COMAR>
        </Provides>
    </Package>

    <Package>
        <Name>firefox-devel</Name>
        <RuntimeDependencies>
            <Dependency>firefox</Dependency>
        </RuntimeDependencies>
        <Files>
            <Path fileType="header">/usr/lib/MozillaFirefox/include</Path>
        </Files>
    </Package>

